{
    "version": "2.0.0",
    "tasks": [{
        "label": "build",
        "command": "egret",
        "type": "shell",
        "presentation": {
            "reveal": "always",
            "panel": "shared",
            "showReuseMessage": true,
            "clear": false
        },
        "args": [
            "build",
            "-sourcemap"
        ],
        "problemMatcher": "$tsc"
    }, {
        "label": "clean",
        "command": "egret",
        "type": "shell",
        "presentation": {
            "reveal": "always",
            "panel": "shared",
            "showReuseMessage": true,
            "clear": false
        },
        "args": [
            "build",
            "-e"
        ],
        "problemMatcher": "$tsc"
    }, {
        "label": "publish",
        "command": "egret",
        "type": "shell",
        "presentation": {
            "reveal": "always",
            "panel": "shared",
            "showReuseMessage": true,
            "clear": false
        },
        "args": [
            "publish"
        ],
        "problemMatcher": "$tsc"
    }, {
        "label": "runEgretServer",
        "command": "egret",
        "type": "shell",
        "presentation": {
            "reveal": "always",
            "panel": "shared",
            "showReuseMessage": true,
            "clear": false
        },
        "args": [
            "run", "-a"
        ],
        "problemMatcher": "$tsc"
